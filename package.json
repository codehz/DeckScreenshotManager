{
  "name": "screenshot-uploader",
  "version": "0.0.3",
  "description": "Screenshot auto uploader because valve don't do it. Upload screenshots to cloud when screenshot is taken.",
  "type": "module",
  "scripts": {
    "build": "shx rm -rf dist && rollup -c",
    "watch": "rollup -c -w",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codehz/DeckScreenshotManager.git"
  },
  "keywords": [
    "decky",
    "plugin",
    "steam-deck",
    "deck",
    "screenshot"
  ],
  "author": "CodeHz <codehz@outlook.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/codehz/DeckScreenshotManager/issues"
  },
  "homepage": "https://github.com/codehz/DeckScreenshotManager#readme",
  "devDependencies": {
    "@decky/rollup": "^1.0.1",
    "@types/react": "16.14.0",
    "@types/webpack": "^5.28.0",
    "react-query": "^3.39.3",
    "shx": "^0.3.4",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@decky/api": "^1.1.2",
    "@decky/ui": "^4.7.1"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom"
      ]
    }
